{
  "name": "@muhammadzadeh/nestorage",
  "version": "0.0.14",
  "description": "NestJS Storage Module",
  "author": "Emir Muhammadzadeh",
  "license": "MIT",
  "url": "https://github.com/muhammadzadeh/nestorage",
  "homepage": "https://github.com/muhammadzadeh/nestorage",
  "repository": {
    "type": "git",
    "url": "https://github.com/muhammadzadeh/nestorage"
  },
  "keywords": [
    "nestjs",
    "object-storage",
    "S3",
    "R2",
    "MINIO",
    "Azure",
    "GCS",
    "GcCloud"
  ],
  "bugs": {
    "url": "https://github.com/muhammadzadeh/nestorage/issues"
  },
  "scripts": {
    "jest": "jest",
    "build": "rimraf -rf dist && tsc -p ./tsconfig.lib.json",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest --config ./tests/jest-e2e.json --runInBand",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-angular": "^18.6.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/testing": "^10.3.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "console-testing-library": "^0.6.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.1.13",
    "release-it": "^17.1.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.521.0",
    "@azure/storage-blob": "^12.17.0",
    "@google-cloud/storage": "^7.7.0"
  }
}